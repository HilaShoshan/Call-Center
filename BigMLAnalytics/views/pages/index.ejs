<div id="contact">
	<div class="contact">
		 
			<div class="container">
				<div class="row contact-row">
				
					<!-- /.contact form -->
					<div class="col-sm-7 contact-right">
						<form method="POST"  onsubmit="predictSubmit(event)" id="contact-form" class="form-horizontal">
							<div class="form-group">
							<input type="text" name="Name" id="Name" class="form-control wow fadeInUp" placeholder="Name" required/>
							</div>
							<div class="form-group">
							<input type="text" name="Email" id="Email" class="form-control wow fadeInUp" placeholder="Email" required/>
							</div>
							<input type="number" name="age" id="Email" class="form-control wow fadeInUp" placeholder="Age" required/>
						    </div>						
							<input type="number" name="pwidth" id="Email" class="form-control wow fadeInUp" placeholder="Age" required/>
						    </div>						
							<input type="number" name="swidth" id="Email" class="form-control wow fadeInUp" placeholder="Age" required/>
						    </div>						
							<input type="number" name="slength" id="Email" class="form-control wow fadeInUp" placeholder="Age" required/>
						    </div>						
							<div class="form-group">
							<textarea name="Message" rows="20" cols="20" id="Message" class="form-control input-message wow fadeInUp"  placeholder="Message" required></textarea>
							</div>
							<div class="form-group">
							<input type="submit" name="submit" value="Submit" class="btn btn-success wow fadeInUp" />
							</div>
						</form>		
