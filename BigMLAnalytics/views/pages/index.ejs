	<!-- /.contact section -->
	<div id="contact">
		<div class="contact">

			<div class="container">
				<div class="row contact-row">
					<h2>Enter a new call's data to predict its topic:</h2>
					<p></p>
					<!-- /.contact form -->
					<div class="col-sm-7 contact-right">
						<form onsubmit="predictSubmit(event)" id="contact-form" class="form-horizontal">
							<div class="form-group">
								<label for="periods">Choose a period:</label>
								<select name="periods" required>
									<option value="holidays">Holidays</option>
									<option value="summer vacations">Summer Vacations</option>
									<option value="normal">Normal</option>
								</select>
							</div>
							<div class="form-group">
								<input type="datetime-local" name="callTime" id="Email"
									class="form-control wow fadeInUp" placeholder="Call Time" required />
							</div>
							<div class="form-group">
								<input type="number" name="numOfCalls" id="Name" class="form-control wow fadeInUp"
									placeholder="Number of calls in the last month" required />
							</div>
							<div class="form-group">
								<input type="checkbox" id="Name" name="internet" value="internet">
								<label for="internet">The customer is interested in internet</label><br>
							</div>
							<div class="form-group">
								<input type="checkbox" id="Name" name="cableTV" value="cableTV">
								<label for="cableTV">The customer is interested in cable TV</label><br>
							</div>
							<div class="form-group">
								<input type="checkbox" id="Name" name="cellular" value="cellular">
								<label for="cellular">The customer is interested in cellular</label>
							</div>
							<div class="form-group">
								<input type="number" name="age" id="Name" min="14" max="120"
									class="form-control wow fadeInUp" placeholder="Customer Age" />
							</div>
							<div class="form-group">
								<label for="genders">Choose customer's gender:</label>
								<select name="genders">
									<option value="male">Male</option>
									<option value="female">Female</option>
								</select>
							</div>
							<div class="form-group">
								<label for="cities">Choose customer's city:</label>
								<select name="cities">
									<option value="Ariel">Ariel</option>
									<option value="Jerusalem">Jerusalem</option>
									<option value="Tel-Aviv">Tel-Aviv</option>
									<option value="Haifa">Haifa</option>
									<option value="Nahariya">Nahariya</option>
									<option value="Petah-Tikva">Petah-Tikva</option>
									<option value="Ashdod">Ashdod</option>
									<option value="Netanya">Netanya</option>
									<option value="Bnei-Brak">Bnei-Brak</option>
									<option value="Holon">Holon</option>
									<option value="Beersheba">Beersheba</option>
								</select>
							</div>
							<div class="form-group">
								<input type="submit" name="submit" value="Submit"
									class="btn btn-success wow fadeInUp" />
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
